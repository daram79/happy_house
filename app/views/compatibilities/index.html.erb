<h1>Listing compatibilities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compatibilities.each do |compatibility| %>
      <tr>
        <td><%= link_to 'Show', compatibility %></td>
        <td><%= link_to 'Edit', edit_compatibility_path(compatibility) %></td>
        <td><%= link_to 'Destroy', compatibility, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Compatibility', new_compatibility_path %>
