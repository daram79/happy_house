<h1>Listing user_covers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_covers.each do |user_cover| %>
      <tr>
        <td><%= link_to 'Show', user_cover %></td>
        <td><%= link_to 'Edit', edit_user_cover_path(user_cover) %></td>
        <td><%= link_to 'Destroy', user_cover, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User cover', new_user_cover_path %>
